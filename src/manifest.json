{
  "name": "Account Commission",
  "description": "Account comission Chrome extension",
  "version": "1.1.0",
  "manifest_version": 2,
  "permissions": ["cookies", "tabs", "activeTab", "https://www.accountrentals.com/api/v1/", "https://extension.test/api/v1/*", "http://ip-api.com/*", "geolocation", "https://*.facebook.com/*", "http://ipinfo.io/*"],
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "browser_action":{"default_title": "Account Comission", "default_popup": "popup/popup.html"},

  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "chrome_style": true
  }
}